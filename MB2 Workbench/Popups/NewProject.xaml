<Window x:Name="NewProject1" x:Class="MB2_Workbench.Popups.NewProject"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MB2_Workbench.Popups"
        mc:Ignorable="d"
        Title="Create New Project" Height="255" Width="490">
    <Grid>
        <ComboBox x:Name="BaseMaps" HorizontalAlignment="Left" Margin="31,64,0,0" VerticalAlignment="Top" Width="409"/>
        <Label Content="Choose a base map" HorizontalAlignment="Left" Height="26" Margin="21,33,0,0" VerticalAlignment="Top" Width="300"/>
        <TextBox HorizontalAlignment="Left" Margin="31,131,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="409"/>
        <Label Content="Map Name" HorizontalAlignment="Left" Height="26" Margin="21,100,0,0" VerticalAlignment="Top" Width="300"/>
        <Button Content="Create" HorizontalAlignment="Left" Margin="31,182,0,0" VerticalAlignment="Top" Width="168" Height="27"/>

    </Grid>
</Window>
